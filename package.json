{
  "name": "lelechart",
  "version": "1.4.3",
  "description": "LeleChart is a React chart library that enables you to create custom charts for your applications.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/antuneslv/lelechart"
  },
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react --no-config",
    "dev": "tsup src/test-dev.tsx --watch",
    "test": "jest",
    "test:cov": "jest --coverage",
    "lint": "eslint src/**/*.ts* --fix"
  },
  "keywords": [
    "chart",
    "react",
    "typescript",
    "javascript"
  ],
  "author": "Leandro Vendemiatto Antunes",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.1.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  }
}
